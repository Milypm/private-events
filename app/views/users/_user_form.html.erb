<%= form_with(model: user) do |f| %>
  <div class="field">
    <% f.label :username %>
    <%= f.text_field :username, autofocus: true, autocomplete: "username", style:"width: 15rem;", placeholder:"Username" %>
  </div>
  <br/>
  <div class="actions">
    <%= f.submit class:"btn btn-success" %>
  </div>
<% end %>